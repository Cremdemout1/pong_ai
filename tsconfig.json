{
    "compilerOptions": {
      "module": "ESNext",
      "moduleResolution": "Node",
      "target": "ES6",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "outDir": "./dist"
    },
    "include": ["src"]
  }
  